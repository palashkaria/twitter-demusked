{
  "manifest_version": 3,
  "name": "Twitter demusked: Remove analytics view count",
  "description": "Chrome Extension, TypeScript, Visual Studio Code",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.twitter.com/*"
  ]
}
